set $mod Mod4
set $left h
set $down j
set $up k
set $right l

set $laptop eDP-1
bindswitch --reload --locked lid:on output $laptop disable
bindswitch --reload --locked lid:off output $laptop enable
output "Goldstar Company Ltd LG HDR 4K 0x00001BEC" transform 90
output "Goldstar Company Ltd LG HDR 4K 0x00001BEC" scale 1.6

include ./config.d/*
include @sysconfdir@/sway/config.d/*